{
  "name": "full-stack-nextjs",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "=20.x.x"
  },
  "scripts": {
    "dev": "next dev",
    "docker:up": "docker-compose up -d && sleep 60",
    "db:init": "env-cmd -f .env npx prisma generate && prisma db push --schema ./prisma/schema.prisma",
    "build:dev": "npx prisma generate && tsc --noEmit",
    "build": "npx prisma generate && prisma db push --schema ./prisma/schema.prisma && next build",
    "start": " next start",
    "lint": "next lint",
    "docker:down": "docker-compose down",
    "exec-db": "docker exec project-postgresql psql -U root -d postgres -c \"CREATE DATABASE $DB_NAME;\"",
    "exec-sql": "pnpm run exec-db && docker cp prisma/db.sql project-postgresql:/db.sql && docker exec project-postgresql psql -U root -d $DB_NAME -f /db.sql"
  },
